/**app.wxss**/
.container {
	position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

.navigator-arrow {
  /* padding-right: 26rpx; */
  right: 26rpx;
  position: absolute;
}
.navigator-arrow::after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: black;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}